on rightclick:
    player has permission "essentials.help"
    if event-block is emerald block:
        if block under event-block is oak fence:
            if player's held item is emerald named "&2&lEmerald Key":
                remove 1 emerald named "&2&lEmerald Key" from player's inventory
                chance of 1%:
                    execute console command "/mm i give -s %player% Event_Token"
